/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */

 /*
 * Mux(a,b,s) = Or(And(a, not(s)), And(b, s))
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
        Not(in=sel , out=nots );
        And(a=a , b=nots , out=aAndnots );
        And(a=b , b=sel , out=bAnds );
        Or(a=aAndnots , b=bAnds , out=out );
}